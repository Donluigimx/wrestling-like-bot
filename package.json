{
  "name": "wrestling-like-bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "engines": {
    "node": "14.16.1"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "refactor",
              "release": "patch"
            },
            {
              "type": "infra",
              "release": "patch"
            }
          ]
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "author": "donluigimx",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@types/node": "^14.14.41",
    "@types/pino": "^6.3.7",
    "@types/redis": "^2.8.28",
    "discord.js": "^12.5.3",
    "ffmpeg-static": "^4.3.0",
    "parse-duration": "^1.0.0",
    "pino": "^6.11.3",
    "redis": "^3.1.1",
    "ytdl-core": "^4.8.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "typescript": "^4.2.4"
  }
}
